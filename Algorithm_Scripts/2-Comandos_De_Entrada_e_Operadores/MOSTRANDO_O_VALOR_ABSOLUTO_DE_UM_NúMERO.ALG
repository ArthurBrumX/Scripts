algoritmo "Mostrando_O_Valor_Absoluto_De_Um_Número"
var

   A: inteiro
   // Nome da Variavel: A
   // Tipo Primitivo: Inteiro
   
inicio

     A <- Abs (-50)
     // A VAriavel (A) vai Receber o valor Absoluto de -50
     
     Escreva (A)// Escreva o valor da variavel (A) na tela
     
fimalgoritmo